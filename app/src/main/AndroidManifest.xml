<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app.recettons">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/mon_icone"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/mon_icone_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.app.recettons.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.app.recettons.Enregistrer" />
        <activity android:name="com.example.app.recettons.Home" />
        <activity android:name="com.example.app.recettons.Controleur" />

        <service
            android:name="com.example.app.recettons.GetRecetteServices"
            android:exported="false" />

        <activity android:name="com.example.app.recettons.RecetteActivity"></activity>
    </application>

</manifest>